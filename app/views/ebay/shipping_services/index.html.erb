<p id="notice"><%= notice %></p>

<h1>Listing Ebay Shipping Services</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ebay_shipping_services.each do |ebay_shipping_service| %>
      <tr>
        <td><%= ebay_shipping_service.name %></td>
        <td><%= link_to 'Show', ebay_shipping_service %></td>
        <td><%= link_to 'Edit', edit_ebay_shipping_service_path(ebay_shipping_service) %></td>
        <td><%= link_to 'Destroy', ebay_shipping_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping service', new_ebay_shipping_service_path %>
